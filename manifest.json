{
  "manifest_version": 2,
  "name": "Push promo",
  "version": "1.0.0",
  "description": "UberFeature",
  "icons": {
    "32": "img/icon.png"
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "UberFeature",
    "default_popup": "index.html"
  },
  "permissions": [
    "gcm",
    "tabs",
    "storage",
    "notifications"
  ]
}
